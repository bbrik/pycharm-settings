<templateSet group="Python">
  <template name="_" value="_(u'$END$')" description="Get text" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML_TEXT" value="false" />
      <option name="HTML" value="false" />
      <option name="XSL_TEXT" value="false" />
      <option name="XML" value="false" />
      <option name="Python" value="true" />
      <option name="Django" value="false" />
      <option name="SQL" value="false" />
      <option name="CSS_PROPERTY_VALUE" value="false" />
      <option name="CSS_DECLARATION_BLOCK" value="false" />
      <option name="CSS_RULESET_LIST" value="false" />
      <option name="CSS" value="false" />
      <option name="JAVA_SCRIPT" value="false" />
      <option name="TypeScript" value="false" />
      <option name="CoffeeScript" value="false" />
      <option name="OTHER" value="false" />
    </context>
  </template>
  <template name="slug" value="r'^(?P&lt;$SLUG$&gt;[\w_-]+)/$" description="slug url part" toReformat="false" toShortenFQNames="true">
    <variable name="SLUG" expression="" defaultValue="slug" alwaysStopAt="true" />
    <context>
      <option name="Python" value="true" />
    </context>
  </template>
  <template name="pk" value="r'^(?P&lt;$PK$&gt;\d+)/$" description="pk url part" toReformat="false" toShortenFQNames="true">
    <variable name="PK" expression="" defaultValue="pk" alwaysStopAt="true" />
    <context>
      <option name="Python" value="true" />
    </context>
  </template>
</templateSet>